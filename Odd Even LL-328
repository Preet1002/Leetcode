class Solution {
public:
    ListNode* oddEvenList(ListNode* head) {
        if(head==NULL) return NULL;
        ListNode* temp=head;
        ListNode* temp2head=head->next;
        ListNode* temp2=head->next;
        while(temp2!=NULL && temp2->next!=NULL){
            temp->next=temp->next->next;
            temp=temp->next;
            temp2->next=temp2->next->next;
            temp2=temp2->next;
        }
        temp->next=temp2head;
        return head;
    }
};
