class Solution {
private:
    double Pow(double x, long long n){
        if(n==0) return 1;
        double half=Pow(x,n/2);
        if(n%2==1){
            return x*half*half;
        }else{
            return half*half;
        }
    }
public:
    double myPow(double x, int N) {
        long long n=N;
        if(n<0){
            n=-n;
            x=1/x;
        }
        double y= Pow(x,n);
        return y;
    }
};
